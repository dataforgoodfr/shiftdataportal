{
  "name": "server",
  "version": "1.0.0",
  "description": "This is the server",
  "main": "server.js",
  "engines": {
    "node": ">=20.11.0"
  },
  "scripts": {
    "start": "node ./dist/server.js",
    "tsc": "tsc",
    "dev": "DEBUG=knex:query ts-node-dev --respawn --transpileOnly ./server.ts",
    "debug": "NODE_ENV=development ts-node-dev --inspect --respawn ./server.ts",
    "generate:types": "cd ../ && yarn generate",
    "build": "yarn tsc",
    "test": "jest --config=./jest.config.js --coverage",
    "generate:db_types": "ts-node-dev ./utils/schempaGenerator.ts"
  },
  "author": "Ulysse Tallepied",
  "license": "ISC",
  "dependencies": {
    "apollo-server-express": "^2.10.1",
    "apollo-server-plugin-response-cache": "^0.3.11",
    "body-parser": "^1.19.0",
    "datasource-sql": "^1.1.1",
    "express": "^4.17.1",
    "graphql": "^14.6.0",
    "knex": "^0.20.10",
    "pg": "^7.18.2",
    "slugify": "^1.4.0"
  },
  "devDependencies": {
    "@types/graphql": "^14.5.0",
    "@types/jest": "^25.1.3",
    "dotenv-flow": "^3.1.0",
    "jest": "^25.1.0",
    "jest-cli": "^25.1.0",
    "jest-junit": "^10.0.0",
    "pg-structure": "^5.10.7",
    "prettier": "^1.19.1",
    "ts-jest": "^25.2.1",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.8.3"
  }
}
